super + w
	$BROWSER
super + semicolon
	$LOCKER
super + Return
	sh -c "$XTERMINAL"
super + z
	color_to_clip
super + p
	sh -c $RUNNER
super + shift + w
	rofi -show window
super + shift + p
	rofi-pass
super + e
	emoji_to_clip
super + s
	genstatus | rofi -dmenu -lines 3 > /dev/null
super + o
	xclip -o -sel c | meme_case | xclip -i -sel c
super + d
	date +%F | tr -d '\n' | xclip -i -sel c
Print
	import -window root "$HOME/Pictures/screenshot/$(date +%F-%T)_screenshot.png"
ctrl + Print
	import "$HOME/Pictures/screenshot/$(date +%F-%T)_screenshot.png"
XF86MonBrightnessDown
	xbacklight -dec 5
XF86MonBrightnessUp
	xbacklight -inc 5
XF86AudioRaiseVolume
	amixer sset 'Master' 5%+
XF86AudioLowerVolume
	amixer sset 'Master' 5%-
XF86AudioMute
	amixer sset 'Master' toggle
ctrl + alt + Delete
	pkill Xorg
super + shift + q
	pkill Xorg
super + shift + r
	pkill -SIGUSR1 sxhkd
